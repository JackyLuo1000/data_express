extends layout

block content
  table(class = 'gridtable')
    tr
      th Username
      th Email
      th Age
      th Sec. Question 1
      th Sec. Question 2
      th Sec. Question 3
      th User Level
    each item in people
      tr
        td
          a(href='/details/' + item.id) #{item.id}
        td #{item.username}
        td #{item.email}
        td #{item.age}
        td #{item.question1}
        td #{item.question2}
        td #{item.question3}
