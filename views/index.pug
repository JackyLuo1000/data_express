extends layout

block content
  table(class = 'gridtable')
    tr
      th Id
      th Username
      th Email
      th Age
      th User Level
      th Q1
      th Q2
      th Q3
      th
      th
    each item in people
      tr
        td
          a(href='/details/' + item.id) #{item.id}
        td #{item.username}
        td #{item.email}
        td #{item.age}
        td #{item.level}
        td #{item.question1}
        td #{item.question2}
        td #{item.question3}
        td
          a(href='/edit/' + item.id) Edit
        td
          a(href='/delete/' + item.id) Delete
          
    p
  #menu
        a(href='/create') Add New User
  br
  canvas(id = 'my_canvas', width='600', height='390')
